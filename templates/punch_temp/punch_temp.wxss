.top_result {
  background-color: red;
  position: absolute;
  display: flex;
  width: 694rpx;
  height: 150rpx;
  top: 68rpx;
  left: 28rpx;
  border: 1px solid #fff;
  border-radius: 10rpx;
}
.top_result .result_item {
  flex: 1;
  text-align: center;
}
.top_result .result_item:nth-child(2) {
  position: relative;
}
.top_result .result_item:nth-child(2) .left {
  position: absolute;
  display: inline-block;
  width: 4rpx;
  height: 56rpx;
  background-color: #E5E5E5;
  left: 0;
  top: 47rpx;
}
.top_result .result_item:nth-child(2) .right {
  position: absolute;
  display: inline-block;
  width: 4rpx;
  height: 56rpx;
  background-color: #E5E5E5;
  right: 0;
  top: 47rpx;
}
.top_result .result_item .number {
  font-size: 40rpx;
  color: #353535;
  font-weight: bold;
  margin-top: 25rpx;
}
.top_result .result_item .name {
  font-size: 12px;
  color: #999999;
}
