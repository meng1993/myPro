 @import '../../templates/courseCell/courseCell.wxss'; 
.list_box {
  width: 750rpx;
  height: 1080rpx;
}
.detail_list {
  width: 722rpx;
  height: 128rpx;
  /* background: red; */
   padding-left: 28rpx; 
  border-bottom: 1px solid #E5E5E5;
}
.detail_list .list_common {
  display: flex;
  width: 100%;
  height: 64rpx;
}
.detail_list .list_common > text {
  flex: 1;
  font-size: 28rpx;
  line-height: 64rpx;
}
.detail_list .list_common text:nth-child(1) {
  color: #353535;
  text-align: left;
}
.detail_list .list_common text:nth-child(2) {
  color: #999999;
  text-align: right;
  margin-right: 28rpx;
}
.detail_list .list_bottom text:nth-child(1) {
  color: #999999;
  text-align: left;
}

