.punch_clock .content .punch_describe {
  position: absolute;
  width: 330rpx;
  height: 90rpx;
  font-size: 28rpx;
  color: #353535;
  text-align: center;
  top: 380rpx;
  left: 210rpx;
}
.punch_clock .content .punch_describe text {
  font-size: 36rpx;
  color: #ec3131;
}
.punch_clock .content .btn_daka {
  position: absolute;
  top: 520rpx;
  left: 113rpx;
  width: 524rpx;
  height: 94rpx;
  font-size: 32rpx;
  color: #FFFFFF;
  text-align: center;
  line-height: 94rpx;
  background: #EE7F00;
  border-radius: 200rpx;
}
.punch_clock .content .hasClick {
  background: rgba(238, 127, 0, 0.4);
}
/*如果是查看奖金池下边内容样式*/
.punch_clock .content .bonus_describe {
  position: absolute;
  width: 400rpx;
  height: 90rpx;
  font-size: 28rpx;
  color: #353535;
  text-align: center;
  top: 380rpx;
  left: 175rpx;
}
.punch_clock .content .bonus_describe text {
  font-size: 36rpx;
  color: #ec3131;
}
.punch_clock .content .scroll_box {
  position: absolute;
  width: 600rpx;
  height: 480rpx;
  top: 450rpx;
}
.punch_clock .content .scroll_box .scroll_title {
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  font-size: 32rpx;
  color: #666666;
}
.punch_clock .content .scroll_box .container {
  width: 100%;
  height: 380rpx;
  font-size: 32rpx;
  color: #666666;
}
.punch_clock .content .scroll_box .container .scroll_content {
  width: 100%;
  height: 100rpx;
  border-bottom: 1px solid lightgray;
}
.punch_clock .content .scroll_box .container .scroll_content image {
  width: 50rpx;
  height: 50rpx;
  float: left;
  border-radius: 50%;
  margin: 25rpx 28rpx 0 0;
}
.punch_clock .content .scroll_box .container .scroll_content .name {
  float: left;
  height: 100rpx;
  line-height: 100rpx;
  font-size: 28rpx;
  color: #353535;
}
.punch_clock .content .scroll_box .container .scroll_content .date {
  float: right;
  height: 100rpx;
  line-height: 100rpx;
  font-size: 28rpx;
  color: #999999;
}
